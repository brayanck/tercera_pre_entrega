<div class="col-md-4 my-auto container">
    <form method="post" action="/auth/register/crear" class="card card-body rounded-0">
        <h1 class="text-center">Register</h1>
        <div class="mb-3">
            <label for="fname" class="form-label">Nombre: </label>
            <input id="fname" name="first_name" type="text" placeholder="nombre" class="form-control">
        </div>
        <div class="mb-3">
            <label for="lname" class="form-label">Apellido:</label>
            <input id="lname" name="last_name" type="text" placeholder="apellifo" class="form-control">
        </div>
        <div class="mb-3">
            <label for="age" class="form-label">Edad:</label>
            <input id="age" name="age" type="number" placeholder="edad" class="form-control">
        </div>
        <div class="mb-3">
            <label for="email" class="form-label">Email:</label>
            <input id="email" name="email" type="text" placeholder="Email" class="form-control">
        </div>
        
        <div class="mb-3">
            <label for="password" class="form-label">Contraseña:</label>
            <input id="password" name="password" type="password" placeholder="Contraseña" class="form-control">
        </div>
        <div class="mb-3 form-check">
            <input type="checkbox" class="form-check-input" id="exampleCheck1">
            <label class="form-check-label" for="exampleCheck1">Check me out</label>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
        <p>ya tenes cuenta<a href="/auth/login">Login</a></p>
    </form>
</div>
